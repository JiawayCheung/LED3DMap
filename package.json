{
  "name": "led-map",
  "description": "A tool to automatically create 3D mappings of lights from two videos",
  "license": "AGPL-3.0-only",
  "repository": "https://github.com/lights0123/LED3DMap",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nuxt-ts -m",
    "build": "nuxt-ts build -m",
    "start": "nuxt-ts start -m",
    "generate": "nuxt-ts generate -m",
    "lint:js": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lint": "yarn lint:js"
  },
  "lint-staged": {
    "*.{js,vue}": "eslint"
  },
  "husky": {
